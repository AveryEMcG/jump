function init_terr()
 terr = {}
	moon = copy(sprite)
	moon.height = 4
	moon.width = 4
	moon.frames = {11}
	moon.pos = {80,10}
	moon.drawd = 8
	
	mountain = copy(sprite)
	mountain.frames ={96}
	mountain.drawd = .5
 mountain.pos ={80, ground-6}
 mountain.width = 4
 mountain.height = 2
 
	add(terr, moon)
	add(terr, mountain) 
	mountain = copy(mountain)
	mountain.pos[1] = 100
	add(terr, mountain) 
	mountain = copy(mountain)
	mountain.pos[1] = 20
	add(terr, mountain) 
end


function addblock(x,y)
	block = copy(sprite)
	block.frames = {23}
	block.drawd = .1
	block.pos ={x,y}
	return block
end


function block_line(x,y,width)

 for i = 0, width do
 	add(blocks,addblock(8*i+x,y))
 end
//add(blocks,addblock(8*i,ground-8))

end


function init_blocks()
	blocks = {}
	block_line(1,ground-8,5)
	block_line(190,ground-40,5)	
		block_line(50,ground-25,3)	
	block_line(100,ground-8,5)	
//	for i = 1, 5 do
//		add(blocks,addblock(8*i,ground-8))
	//end
	
end

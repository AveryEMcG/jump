activekeys = {false,false,false,false}
left = 1
right = 2
up = 3
down = 4
accel = {0,0}
accelmax = 5
accel_speed = 1
ppos = {50,112}
friction = .25
playersprites = {7,8,7,9}
currsprite={1,time()}
grav = 1
maxgrav = -8
ground = 112
//falling = false
bg_pos = {0,0}
startjump = false
bumpsfx = false
buttond =
{
 false,
 false,
 false,
 false
}

function btnd(id) 
	if (btn(id)==false) then
		buttond[id] = false
	else
		if buttond[id] then
			return false
		else
		 buttond[id] = true
		 return true
		end
	end
end

function handle_buttons()
 activekeys =
 {
 btn(0),
 btn(1),
 btnd(2),
 btn(3)
 }
end




function update_movement()
 
 if activekeys[right] then
  if (accel[1] ==accelmax-friction)and grounded()
  	then sfx(1)
  end
  accel[1] -= accel_speed


  faceleft = false

 elseif activekeys[left] then
  if (accel[1] ==-accelmax+friction)and grounded()
  	then sfx(1)
  end
  accel[1] += accel_speed
  faceleft = true
 end
 if ((activekeys[up]) and (grounded()!=false) )then
 	accel[2] +=16
 	startjump = true
 	sfx(3)
 end
 cap_accel()

 gravity()
 apply_accel()
end

function gravity()	
	accel[2]-=grav
	
	if accel[2]<= maxgrav then
		accel[2] = maxgrav
	end

 //add(debug, "accel: "..accel[2]) 
 //add(debug, "pos("..ppos[1]..','..ppos[2]..")")
end

function cap_accel()
 if accel[1] > accelmax then
 	accel[1] = accelmax
 elseif accel[1] < (accelmax*-1)then
  accel[1] = (accelmax*-1)
 end
 
 if accel[1] > 0 then 
	 accel[1]-=friction
 elseif accel[1] < 0 then
 	accel[1]+=friction
end
 
end

function apply_accel()

	g = grounded()
	b = blocked(ppos[2]-accel[2])
	if g and (not startjump) then
		accel[2] = maxgrav
	 ppos[2] = g
		bumpsfx = false
	elseif b
	then
		accel[2] = 0
		if bumpsfx==false then
			sfx(0)
			bumpsfx = true
		end
		ppos[2] =b
	else
	 bumpsfx = false
		ppos[2]-=accel[2]
		startjump = false
 end
 
 //pos[2] += accel[2]
 bg_pos[1] += accel[1]/25
 
 

end


faceleft = false
function drawplayer()

 local newtime = time()
 local u = newtime-currsprite[2]
	local accelab = abs(accel[1])

	if (accelab==0) then
		u = nil
		currsprite[1] = 1
	elseif (u > .5/accelab) then
 	currsprite[1]+=1
  if currsprite[1]>#playersprites then
  currsprite[1] = 1
  end
  if (playersprites[currsprite[1]] == 7 )
  and (grounded())then
  	sfx(2)
  end
	
  currsprite[2] = newtime
 end
 spr(playersprites[currsprite[1]],50,ppos[2],1,1,faceleft)


end


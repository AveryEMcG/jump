sprite =
{
 frames = {1},
 currframe = 1,
 faceleft = false,
 width = 1,
 height = 1,
 pos = {0,0},
 drawd = 1
}

function sprite:draw()
	local xpos = bg_pos[1]/self.drawd + self.pos[1]
	local ypos = self.pos[2]
	if 
		(xpos >128)
		or 
		(xpos+(self.width*8) <0)
		or
		(ypos >128)
		or
		(ypos+(self.height*8) <0) then
			return
	else
 	spr(
 	self.frames[self.currframe],
 	xpos,
 	ypos,
 	self.width, 
 	self.height, 
 	false, 
 	self.faceleft)
 end
end

function sprite:update()
	self.currframe +=1
	if self.currframe>#self.frames then
		self.currframe = 1
	end	
end
function sprite:underplayer()
 local xpos = bg_pos[1]/self.drawd + self.pos[1]
	local ypos = self.pos[2]
 if 
 ((xpos <= 58) and (xpos >= 42))
 and
 (ppos[2] >= ypos-10) and
 (ppos[2] <= ypos)  then
 	return  ypos-8
 else
  return nil
 end
end
function sprite:blocked(y)
 local xpos = bg_pos[1]/self.drawd + self.pos[1]
	local ypos = self.pos[2]

 if 
 ((xpos <= 58) and (xpos >= 42))
 and
 (y >= ypos) and
 (y <= ypos+8)  then
 	return  ypos+8
 	
 end
 return false
	
	
end





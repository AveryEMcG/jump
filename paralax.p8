function grounded()
	if ppos[2] >= ground then
	 return ground
	else
		return	land()
	end
end


function blocked(y)
 for i =1, #blocks do
  b = blocks[i]:blocked(y)
 	if b !=false  then
 		return b
 	end
 end
 return false
end 

